<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anubhavbhardwaj.watchyourdiabetes">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditableInformation" />
        <activity android:name=".EditableInformation2" />
        <activity android:name=".SetupAppointment" />
        <activity android:name=".Reminders"></activity>
        <receiver android:name="AlarmReceiver" android:process=":remote"></receiver>
    </application>

</manifest>